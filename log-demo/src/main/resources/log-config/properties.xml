<!--变量-->
<Properties>
    <!--输出格式  默认%m%n-->
    <Property name="LOG_INFO_PATTERN">%style{%d{yyyy-MM-dd'T'HH:mm:ss.SSS}}{cyan} %highlight{%p} %style{%m%n}{bright}</Property>
    <Property name="LOG_ERROR_PATTERN">%style{%d{yyyy-MM-dd'T'HH:mm:ss.SSS}}{cyan} %highlight{%p} %style{%m %l %n}{bright}</Property>
    <!--输出目录 -->
    <Property name="LOG_PATH">./logs</Property>
</Properties>


<!--%p 等级-->
<!--%d 时间-->
<!--%r 自应用启动的毫秒数-->
<!--%c 日志信息所属类的全名-->
<!--%t 线程名-->
<!--%l 日志发生位置 相当于%C.%M(%F:%L)-->
<!--%F 文件名称-->
<!--%L 代码行号-->
<!--%m 具体信息-->
<!--%n 换行-->